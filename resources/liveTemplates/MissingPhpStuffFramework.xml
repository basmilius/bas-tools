<templateSet group="Identity Framework">
	<template name="idtyc" value="Identity::core()-&gt;$END$" description="Identity Framework - Core" toReformat="true" toShortenFQNames="true">
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idtyp" value="Identity::plugin($PLUGINCLASS$::class, function ($PLUGINCLASS$ $PLUGINNAME$)&#10;{&#10;&#9;$END$&#10;});" description="Identity Framework - Plugin call" toReformat="true" toShortenFQNames="true">
		<variable name="PLUGINCLASS" expression="classNameComplete()" defaultValue="&quot;IdtyPlugin&quot;" alwaysStopAt="true" />
		<variable name="PLUGINNAME" expression="phpSuggestFunctionParameter()" defaultValue="&quot;plugin&quot;" alwaysStopAt="true" />
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idtyb" value="Identity::bootstrap()-&gt;$END$" description="Identity Framework - Bootstrap" toReformat="true" toShortenFQNames="true">
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idty" value="Identity::$END$" description="Identity Framework" toReformat="true" toShortenFQNames="true">
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idtyle" value="Identity::core()-&gt;loadExtension($EXTENSION$::class);$END$" description="Identity Framework - Load extension" toReformat="true" toShortenFQNames="true">
		<variable name="EXTENSION" expression="classNameComplete()" defaultValue="&quot;IdtyExtension&quot;" alwaysStopAt="true" />
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idtyt" value="Identity::template()-&gt;$END$" description="Identity Framework - Template" toReformat="true" toShortenFQNames="true">
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="iaction" value="Identity::action('$ACTION$', function ($ARGUMENTS$)&#10;{&#10;&#9;$END$&#10;}, $PRIORITY$, $ARGUMENTCOUNT$);" description="Identity Framework - Hook action" toReformat="true" toShortenFQNames="true">
		<variable name="ACTION" expression="" defaultValue="&quot;action&quot;" alwaysStopAt="true" />
		<variable name="ARGUMENTS" expression="phpSuggestFunctionParameter()" defaultValue="&quot;&quot;" alwaysStopAt="true" />
		<variable name="PRIORITY" expression="" defaultValue="&quot;10&quot;" alwaysStopAt="true" />
		<variable name="ARGUMENTCOUNT" expression="" defaultValue="&quot;1&quot;" alwaysStopAt="true" />
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="ifilter" value="Identity::filter('$FILTER$', function ($ARGUMENTS$) : $RETURNTYPE$&#10;{&#10;&#9;return $END$;&#10;}, $PRIORITY$, $ARGUMENTCOUNT$);" description="Identity Framework - Hook filter" toReformat="true" toShortenFQNames="true">
		<variable name="FILTER" expression="" defaultValue="&quot;filter&quot;" alwaysStopAt="true" />
		<variable name="ARGUMENTS" expression="phpSuggestFunctionParameter()" defaultValue="" alwaysStopAt="true" />
		<variable name="RETURNTYPE" expression="" defaultValue="&quot;array&quot;" alwaysStopAt="true" />
		<variable name="PRIORITY" expression="" defaultValue="&quot;10&quot;" alwaysStopAt="true" />
		<variable name="ARGUMENTCOUNT" expression="" defaultValue="&quot;1&quot;" alwaysStopAt="true" />
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idtyr" value="Identity::registry()-&gt;$END$" description="Identity Framework - Registry" toReformat="false" toShortenFQNames="true">
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idtyrg" value="Identity::registry()-&gt;get('$REGISTRYKEY$');$END$" description="Identity Framework - Registry - Get" toReformat="true" toShortenFQNames="true">
		<variable name="REGISTRYKEY" expression="" defaultValue="&quot;registry.key&quot;" alwaysStopAt="true" />
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idtyrs" value="Identity::registry()-&gt;set('$REGISTRYKEY$', $REGISTRYVALUE$);$END$" description="Identity Framework - Registry - Set" toReformat="true" toShortenFQNames="true">
		<variable name="REGISTRYKEY" expression="" defaultValue="&quot;registry.key&quot;" alwaysStopAt="true" />
		<variable name="REGISTRYVALUE" expression="" defaultValue="&quot;null&quot;" alwaysStopAt="true" />
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
	<template name="idtya" value="Identity::ajax('$ACTION$', $CALLBACK$, $PRIORITY$);$END$" description="Identity Framework - Ajax" toReformat="false" toShortenFQNames="true">
		<variable name="ACTION" expression="" defaultValue="&quot;action&quot;" alwaysStopAt="true" />
		<variable name="CALLBACK" expression="" defaultValue="&quot;callable&quot;" alwaysStopAt="true" />
		<variable name="PRIORITY" expression="" defaultValue="&quot;10&quot;" alwaysStopAt="true" />
		<context>
			<option name="BASTOOLS.IDENTITY_FRAMEWORK" value="true" />
		</context>
	</template>
</templateSet>